{
  "stylelint.validate": [
    "vue"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.options": {
    "extensions": [
      ".html",
      ".js",
      ".ts",
      ".vue",
      ".jsx",
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptvue",
    "typescript",
    "typescriptvue",
    "html",
    "vue",
  ],
}
